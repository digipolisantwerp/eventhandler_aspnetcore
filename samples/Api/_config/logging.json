{
    "ConsoleLogging": 
    {
        "IncludeScopes": false,
        "LogLevel": 
        {
            "Default": "Verbose",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "SeriLog": {
        "MinimumLevel": "Information",
        "RollingLogFile": {
            "MinimumLevel": "Information",
            "Path": "c:\\temp\\sampleapi-{Date}.log",
            "OutputTemplate": "{Date:yyyy-MM-dd} {Timestamp:HH:mm:ss.fff} [{Level}] ({ThreadId}) {Message}"
        }
    }
}